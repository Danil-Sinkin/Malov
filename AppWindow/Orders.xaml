<Window x:Class="AppWindow.Orders"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppWindow"
        mc:Ignorable="d"
        Title="Orders" Height="450" Width="800">
    <Grid>
        <ListBox HorizontalAlignment="Center" Height="335" Margin="0,23,0,0" VerticalAlignment="Top" Width="730" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Arial Black" FontSize="20" SelectionChanged="listOrders"/>
        <Button Content="Вернуться" HorizontalAlignment="Left" Height="46" Margin="35,363,0,0" VerticalAlignment="Top" Width="711" Background="{x:Null}" BorderBrush="Black" OpacityMask="Black" FontSize="20" FontFamily="Arial Black" Click="backOrders"/>
    </Grid>
</Window>
